    "portfinder": "^1.0.32",
    "postcss": "^8.4.24",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.8.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-obfuscator": "^1.0.3",
    "sass": "^1.62.1",
    "socket.io-client": "^4.6.1",
    "terser": "^5.30.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-top-level-await": "^1.3.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.6.2",
    "vue-router": "^4.1.6",
    "zrender": "^5.5.0"
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs}": "eslint --cache --fix",
    "*.src/": "prettier --write"
  },
  "__npminstall_done": false
}
